{
  "functions": {
    "app/api/upload/route.ts": {
      "maxDuration": 1800
    },
    "app/api/debug/route.ts": {
      "maxDuration": 30
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" }
  ],
  "regions": ["iad1"],
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": ".next"
}